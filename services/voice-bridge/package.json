{
  "name": "aigents-voice-bridge",
  "version": "1.0.0",
  "description": "AIGENTS-integrated voice agent for heart-failure outreach using OpenAI Realtime API over Twilio Media Streams",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/server.ts",
    "build": "tsc -p .",
    "start": "node dist/server.js",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate"
  },
  "keywords": [
    "aigents",
    "voice",
    "telephony",
    "twilio",
    "openai",
    "realtime",
    "healthcare"
  ],
  "author": "jeffbander",
  "license": "MIT",
  "dependencies": {
    "@neondatabase/serverless": "^0.7.2",
    "argon2": "^0.31.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pino": "^8.16.1",
    "pino-pretty": "^10.2.3",
    "rate-limiter-flexible": "^3.0.6",
    "socket.io": "^4.8.1",
    "twilio": "^4.20.0",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "@types/ws": "^8.5.8",
    "drizzle-kit": "^0.20.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
